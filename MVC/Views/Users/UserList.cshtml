@{
}
@using Entity.Dtos;
@model List<UsersWtihLocation>


<div style="overflow-x:auto;">
    <table>
        <tr>
            <th> Name</th>
            <th>Surname</th>
            <th>Mail</th>
            <th>Phone</th>
            <th>Branch Name</th>
            <th>Department Name</th>
        </tr>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.Name</td>
                    <td>@user.Surname</td>
                    <td>@user.Email</td>
                    <td>@user.Phone</td>
                    <td>@user.BranchName</td>
                    <td>@user.DepartmentName</td>
                </tr>
            }
        </tbody>

    </table>
</div>
