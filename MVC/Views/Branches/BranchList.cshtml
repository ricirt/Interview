@{
}
@using Entity;
@model List<Branch>
<div style="overflow-x:auto;">
    <table>
        <tr>
            <th> Name</th>
            <th>Surname</th>
            <th>Mail</th>
            <th>Phone</th>
            <th>Branch Name</th>
            <th>Department Name</th>
        </tr>
        <tbody>
            @foreach (var branch in Model)
            {
                <tr>
                    <td>@branch.</td>

                </tr>
            }
        </tbody>

    </table>
</div>
